{
  "manifest_version": 3,
  "name": "Crisp Gemini Summarizer",
  "version": "1.0",
  "description": "Summary",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://app.crisp.chat/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png", 
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}